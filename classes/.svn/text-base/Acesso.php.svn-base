		{
			$tipoAutenBanda = 2;
		}
		if($rowAutenBandaInterface['SIMPLES_QUEUE'] == 1)
		{
			$tipoAutenBanda = 3;
		}
		if($rowAutenBandaInterface['HOTSPOT_RADIUS'] == 1)
		{
			$tipoAutenBanda = 4;
		}
		
			if ($API->connect($ip, $usuario, $senha)) {					
		
				if($tipoAutenBanda == 1)
				{				
					  //PROCURA USUARIO NO HOTSPOT;
					  $API->write('/ip/hotspot/user/print',false);
					  $API->write('?name=' . $login);
